{
  "name": "@lit/lit-starter-ts",
  "private": true,
  "version": "0.0.0",
  "description": "cowinator web app",
  "main": "cowin-app.js",
  "module": "cowin-app.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "wds --watch --node-resolve --app-index index.html --open"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Nakshathru Ajay",
  "dependencies": {
    "@vaadin/router": "^1.7.4",
    "lit": "^2.0.0-rc.1"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.2.7",
    "@web/test-runner": "^0.12.15",
    "@web/test-runner-playwright": "^0.8.4",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "eslint": "^6.8.0",
    "lit-analyzer": "^1.1.10",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-summary": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^3.8.3",
    "web-component-analyzer": "^1.0.3"
  }
}
